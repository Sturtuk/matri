ul.menu { float:right; list-style:none; padding:0; margin:0;}



ul.menu li{ float:left; padding:0; margin:0 6px; position:relative; }



ul.menu li a:link, ul.menu li a:visited{ display:block; padding:3px 9px; font-size:16px; color:#383838;}



ul.menu li a:hover,ul.menu li.active a,ul.menu li a.menuhover { color:#fff; padding:3px 9px; /*border:1px solid #71b1cc;*/ -webkit-border-radius: 3px; -moz-border-radius: 3px; border-radius: 3px; background:#0082b9; border-radius:3px; -moz-border-radius:3px; -webkit-border-radius:3px; /*background:url(../images/menu_bg.png) repeat;*/}



ul.menu li a span{   background: none repeat scroll 0 0 #F87392;    border-radius: 4px 4px 4px 4px;    color: #FFFFFF;    font-size: 11px;    height: 15px;



    line-height: 11px;    margin-left: 3px;    padding: 0 5px;}



ul.menu li a:hover span,ul.menu li.active a span,ul.menu li a.menuhover span{margin-top:-1px;}



ul.menu ul {position:absolute; top:27px; left:0;  display:none; list-style:none; z-index:1; margin:0; padding:0; width:159px; }



ul.menu ul div {  background: #FFFFFF;    border: 2px solid #81C6E3;    border-radius: 0 10px 10px 10px;    float: left;



    overflow: hidden;} 



ul.menu ul li{position:relative; margin:0; font-size:12px; font-weight:normal; padding:0; z-index:1; border-top:1px solid #81C6E3; }



ul.menu ul li:first-child{border-top:none;}



ul.menu ul li a:link, ul.menu ul li a:visited { display:block; margin:0px; padding:5px; color:#267896; text-decoration:none; font-size:12px; background:none; height:auto; width:145px; font-weight:normal; line-height:22px; z-index:1; border:none; text-transform:none; -webkit-border-radius: 0px; -moz-border-radius: 0px; border-radius: 0px; }



ul.menu ul li a:hover { background:#58b1d4;color:#fff; border:none;}



ul.menu ul ul {left:155px; top:0px;}



